@model Book

<div class="card">
    <div class="card-header">
        Book Data
    </div>
    <div class="card-body">
        <blockquote class="blockquote mb-0">
            <h2 class="card-title"> <small>Title Book:-       </small> @Model.Title</h2>
            <h3 class="card-text">  <small>Author Name:-      </small> @Model.Author</h3>
            <h5 class="card-text">  <small>Langauge Of Book:-      </small> @Model.LangOfBook</h5>
            <h5 class="card-text">  <small>Category Name:-  </small> @Model.CatgID</h5>
        </blockquote>
    </div>
</div>



